{
	"name": "@ritzlgrmft/ionic-configuration-service",
	"version": "1.0.0",
	"description": "Configuration functionalities",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-configuration-service.git",
	"license": "MIT",
	"private": false,
	"scripts": {
		"clean": "gulp clean",
		"commit": "git-cz",
		"lint": "tslint --project tsconfig.json --format verbose",
		"precommit": "npm run test-once",
		"prepare-release": "gulp prepare-release",
		"test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
		"test-once": "tsc && karma start karma.conf.js --single-run",
		"tsc": "tsc"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"dependencies": {
		"@angular/core": "2.2.1",
		"rxjs": "5.0.0-beta.12",
		"zone.js": "0.6.26"
	},
	"devDependencies": {
		"@angular/common": "2.2.1",
		"@angular/compiler": "2.2.1",
		"@angular/forms": "2.2.1",
		"@angular/http": "2.2.1",
		"@angular/platform-browser": "2.2.1",
		"@angular/platform-browser-dynamic": "2.2.1",
		"@types/es6-shim": "0.31.32",
		"@types/jasmine": "2.5.37",
		"browserify": "13.1.1",
		"browserify-istanbul": "2.0.0",
		"codecov.io": "0.1.6",
		"codelyzer": "1.0.0-beta.4",
		"commitizen": "2.9.3",
		"cz-conventional-changelog": "1.2.0",
		"concurrently": "3.1.0",
		"del": "2.2.2",
		"gulp": "3.9.1",
		"husky": "0.12.0",
		"isparta": "4.0.0",
		"jasmine-core": "2.5.2",
		"karma": "1.3.0",
		"karma-browserify": "5.1.0",
		"karma-chrome-launcher": "2.0.0",
		"karma-coverage": "1.1.1",
		"karma-jasmine": "1.0.2",
		"karma-jasmine-html-reporter": "0.2.2",
		"karma-mocha-reporter": "2.2.0",
		"karma-phantomjs-launcher": "1.0.2",
		"karma-requirejs": "^1.1.0",
		"phantomjs-prebuilt": "2.1.13",
		"reflect-metadata": "0.1.8",
		"run-sequence": "1.2.2",
