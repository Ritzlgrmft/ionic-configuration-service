{
	"name": "@ritzlgrmft/ionic-configuration-service",
	"version": "1.0.0",
	"description": "Configuration functionalities",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-configuration-service.git",
	"license": "MIT",
	"private": false,
	"scripts": {
		"build": "npm run clean && npm run tsc",
		"clean": "rm -rf dist",
		"commit": "git-cz",
		"lint": "tslint --project tsconfig.json --format verbose",
		"precommit": "npm run validate",
		"test": "karma start karma.conf.js",
		"test-once": "karma start karma.conf.js --single-run",
		"tsc": "npm run tsc-commonjs && npm run tsc-es2015",
		"tsc-commonjs": "tsc",
		"tsc-es2015": "tsc --module es2015 --outDir ./dist/es2015",
		"validate": "npm run test-once && npm run lint"
	},
	"files": [
		"dist"
	],
	"main": "dist/es2015/index.js",
	"typings": "dist/es2015/index.d.ts",
	"dependencies": {
		"@angular/core": "2.2.1",
		"@angular/http": "2.2.1",
		"rxjs": "5.0.0-beta.12",
		"zone.js": "0.6.26"
	},
	"devDependencies": {
		"@angular/common": "2.2.1",
		"@angular/compiler": "2.2.1",
		"@angular/forms": "2.2.1",
		"@angular/platform-browser": "2.2.1",
		"@angular/platform-browser-dynamic": "2.2.1",
		"@types/es6-shim": "0.31.32",
		"@types/jasmine": "2.5.40",
		"codecov.io": "0.1.6",
		"codelyzer": "1.0.0-beta.4",
		"commitizen": "2.9.3",
		"cz-conventional-changelog": "1.2.0",
		"concurrently": "3.1.0",
		"husky": "0.12.0",
		"jasmine-core": "2.5.2",
		"karma": "1.3.0",
		"karma-chrome-launcher": "2.0.0",
		"karma-jasmine": "1.1.0",
		"karma-typescript": "2.1.5",
		"reflect-metadata": "0.1.8",
		"requirejs": "2.3.2",
		"tslint": "3.15.1",
		"typescript": "2.0.9"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}