{
	"name": "ionic-configuration-service",
	"version": "0.0.0-development",
	"description": "Configuration functionalities for apps built with Ionic framework",
	"author": {
		"name": "Markus Wagner",
		"email": "markus.wagner02@web.de"
	},
	"repository": "https://github.com/Ritzlgrmft/ionic-configuration-service.git",
	"license": "MIT",
	"private": false,
	"scripts": {
		"build": "npm run clean && npm run tsc",
		"clean": "rm -rf dist",
		"commit": "git-cz",
		"report-coverage": "cp coverage/PhantomJS*/lcovonly coverage/lcov.info && codecov",
		"karma": "karma start karma.conf.js --browsers Chrome",
		"karma-once": "karma start karma.conf.js --browsers PhantomJS --single-run",
		"lint": "tslint --project tsconfig.json --format verbose --type-check",
		"test": "npm run tsc && npm run lint && npm run karma-once",
		"tsc": "npm run tsc-commonjs && npm run tsc-es2015",
		"tsc-commonjs": "tsc",
		"tsc-es2015": "tsc --module es2015 --outDir ./dist/es2015",
		"validate": "npm run karma-once && npm run lint",
		"semantic-release": "semantic-release pre && npm publish && semantic-release post"
	},
	"files": [
		"dist"
	],
	"main": "dist/es2015/index.js",
	"typings": "dist/es2015/index.d.ts",
	"dependencies": {
		"@angular/core": "2.2.1",
		"@angular/http": "2.2.1",
		"rxjs": "5.0.0-beta.12",
		"zone.js": "0.6.26",
		"@types/es6-shim": "0.31.32"
	},
	"devDependencies": {
		"@angular/common": "2.2.1",
		"@angular/compiler": "2.2.1",
		"@angular/forms": "2.2.1",
		"@angular/platform-browser": "2.2.1",
		"@angular/platform-browser-dynamic": "2.2.1",
		"@types/jasmine": "2.5.40",
		"codecov": "1.0.1",
		"codelyzer": "2.0.0-beta.4",
		"commitizen": "2.9.4",
		"cz-conventional-changelog": "1.2.0",
		"concurrently": "3.1.0",
		"husky": "0.12.0",
		"jasmine-core": "2.5.2",
		"karma": "1.3.0",
		"karma-chrome-launcher": "2.0.0",
		"karma-es6-shim": "1.0.0",
		"karma-jasmine": "1.1.0",
		"karma-phantomjs-launcher": "1.0.2",
		"karma-typescript": "2.1.5",
		"reflect-metadata": "0.1.8",
		"requirejs": "2.3.2",
		"tslint": "4.3.1",
		"typescript": "2.0.9",
		"semantic-release": "6.3.2"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	}
}